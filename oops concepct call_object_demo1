class arithmetic_op:
    def sum(self,a,b):
        return a+b
    def mul(self,p,q):
        return p*q
    def sub(self,a,b):
        return a-b
    def division(self,a,b):
        return a/b
    def find_even(self,list):
        for i in list:
            if i%2==0:
                print(i,end=" ")
    def fact(self,a):
        res=1
        for i in range(1,a+1):
            res=res*1
            return res
